    "react-onclickoutside": "^6.7.1",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.4.3",
    "style-loader": "^0.22.1",
    "webpack": "^4.16.5",
    "webpack-command": "^0.4.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
